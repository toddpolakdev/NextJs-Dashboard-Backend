#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/todd/Documents/repos/NextJs-Dashboard-Backend/node_modules/.pnpm/micro@9.4.1/node_modules/micro/bin/node_modules:/Users/todd/Documents/repos/NextJs-Dashboard-Backend/node_modules/.pnpm/micro@9.4.1/node_modules/micro/node_modules:/Users/todd/Documents/repos/NextJs-Dashboard-Backend/node_modules/.pnpm/micro@9.4.1/node_modules:/Users/todd/Documents/repos/NextJs-Dashboard-Backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/todd/Documents/repos/NextJs-Dashboard-Backend/node_modules/.pnpm/micro@9.4.1/node_modules/micro/bin/node_modules:/Users/todd/Documents/repos/NextJs-Dashboard-Backend/node_modules/.pnpm/micro@9.4.1/node_modules/micro/node_modules:/Users/todd/Documents/repos/NextJs-Dashboard-Backend/node_modules/.pnpm/micro@9.4.1/node_modules:/Users/todd/Documents/repos/NextJs-Dashboard-Backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../micro@9.4.1/node_modules/micro/bin/micro.js" "$@"
else
  exec node  "$basedir/../../../../../../micro@9.4.1/node_modules/micro/bin/micro.js" "$@"
fi
